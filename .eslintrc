{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": [
    "standard-with-typescript",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "rules": {
    "import/order": ["error", {"newlines-between": "always-and-inside-groups"}]
  },
  "ignorePatterns": [
    "lib/**"
  ]
}
